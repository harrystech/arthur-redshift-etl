{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "console": {
            "format": "%(asctime)s - %(levelname)s - %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        },
        "file": {
            "format": "%(asctime)s %(trace_key)s %(levelname)s %(name)s (%(threadName)s) [%(filename)s:%(lineno)d] %(message)s"
        }
    },
    "filters": {
        "insert_trace_key": {
            "()": "etl.monitor.InsertTraceKey"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "console",
            "level": "INFO",
            "stream": "ext://sys.stderr",
            "filters": [ "insert_trace_key" ]
        },
        "file": {
            "class": "logging.handlers.RotatingFileHandler",
            "formatter": "file",
            "level": "DEBUG",
            "filename": "arthur.log",
            "mode": "a",
            "maxBytes": 5000000,
            "backupCount": 5,
            "filters": [ "insert_trace_key" ]
        }
    },
    "root": {
        "handlers": [ "console", "file" ],
        "level": "NOTSET"
    },
    "loggers": {
        "botocore": {
            "qualname": "botocore",
            "handlers": [ "file" ],
            "level": "INFO",
            "propagate": 0
        },
        "botocore.credentials": {
            "qualname": "botocore.credentials",
            "handlers": [ "file" ],
            "level": "DEBUG",
            "propagate": 0
        },
        "boto3": {
            "qualname": "boto3",
            "handlers": [ "file" ],
            "level": "INFO",
            "propagate": 0
        },
        "s3transfer": {
            "qualname": "s3transfer",
            "handlers": [ "file" ],
            "level": "INFO",
            "propagate": 0
        },
        "py4j.java_gateway": {
            "qualname": "py4j.java_gateway",
            "handlers": [ "file" ],
            "level": "INFO",
            "propagate": 0
        }
    }
}
